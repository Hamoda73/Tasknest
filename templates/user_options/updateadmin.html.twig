{% extends 'baseadmin.html.twig' %}

{% block stylesheets %}
	<link href="../css/mycss.css" rel="stylesheet">
	<link
	rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
	<!-- Font Awesome -->
	<link rel="stylesheet" href="../css/admin-dashboard/all.min.css">
	<link
	rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
	<!-- Ionicons -->
	<link
	rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
	<!-- Tempusdominus Bootstrap 4 -->
	<link
	rel="stylesheet" href="../css/admin-dashboard/tempusdominus-bootstrap-4.min.css">
	<!-- iCheck -->
	<link
	rel="stylesheet" href="../css/admin-dashboard/icheck-bootstrap.min.css">
	<!-- JQVMap -->
	<link
	rel="stylesheet" href="../css/admin-dashboard/jqvmap.min.css">
	<!-- Theme style -->
	<link
	rel="stylesheet" href="../css/admin-dashboard/adminlte.min.css">
	<!-- overlayScrollbars -->
	<link
	rel="stylesheet" href="../css/admin-dashboard/OverlayScrollbars.min.css">
	<!-- Daterange picker -->
	<link
	rel="stylesheet" href="../css/admin-dashboard/daterangepicker.css">
	<!-- summernote -->
	<link rel="stylesheet" href="../css/admin-dashboard/summernote-bs4.min.css">
{% endblock %}
<style>
	.custom-label {
		color: blue!important;
	}
</style>
{% block javascripts %}
	<!-- jQuery -->
	<script src="{{asset('../js/plugins/jquery/jquery.min.js')}}"></script>
	<!-- jQuery UI 1.11.4 -->
	<script src="{{asset('../js/plugins/jquery-ui/jquery-ui.min.js')}}"></script>
	<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
	<script>
		$.widget.bridge('uibutton', $.ui.button)
	</script>
	<!-- Bootstrap 4 -->
	<script src="{{asset('../js/plugins/bootstrap/js/bootstrap.bundle.min.js')}}"></script>
	<!-- ChartJS -->
	<script src="{{asset('../js/plugins/chart.js/Chart.min.js')}}"></script>
	<!-- Sparkline -->
	<script src="{{asset('../js/plugins/sparklines/sparkline.js')}}"></script>
	<!-- JQVMap -->
	<script src="{{asset('../js/plugins/jqvmap/jquery.vmap.min.js')}}"></script>
	<script src="{{asset('../js/plugins/jqvmap/maps/jquery.vmap.usa.js')}}"></script>
	<!-- jQuery Knob Chart -->
	<script src="{{asset('../js/plugins/jquery-knob/jquery.knob.min.js')}}"></script>
	<!-- daterangepicker -->
	<script src="{{asset('../js/plugins/moment/moment.min.js')}}"></script>
	<script src="{{asset('../js/plugins/daterangepicker/daterangepicker.js')}}"></script>
	<!-- Tempusdominus Bootstrap 4 -->
	<script src="{{asset('../js/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js')}}"></script>
	<!-- Summernote -->
	<script src="{{asset('../js/plugins/summernote/summernote-bs4.min.js')}}"></script>
	<!-- overlayScrollbars -->
	<script src="{{asset('../js/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js')}}"></script>
	<!-- AdminLTE App -->
	<script src="{{asset('../js/dist/js/adminlte.js')}}"></script>
	<!-- AdminLTE for demo purposes -->
	<script src="{{asset('../js/dist/js/demo.js')}}"></script>
	<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
	<script src="{{asset('../js/dist/js/pages/dashboard.js')}}"></script>
{% endblock %}

{% block body %}
	<section class="explore-section section-padding" id="section_2">
		<div class="dashboard-managment-table">
			<div class="row">
				<div class="container">
					<div class="col-lg-6 ">
						<div class="formcustom-form" id="signup-form">
							{{ form_start(form,{attr:{novalidate:'novalidate'}}) }}
							<div class="margin-inputs xx">
								<div class="xx form-floating label-width">
									{{ form_widget(form.fname,{'attr':{'class':'form-control'}}) }}
									{{ form_errors(form.fname) }}
									<label class="custom-label" for="floatingInput">First Name</label>
								</div>

								<div class="xx form-floating label-width">
									{{ form_widget(form.lname,{'attr':{'class':'form-control'}}) }}
									{{ form_errors(form.fname) }}
									<label class="custom-label" for="floatingInput">Last Name</label>
								</div>
							</div>
							<div class="margin-inputs xx">
								<div class="xx form-floating label-width">
									{{ form_widget(form.birthdate, {'attr': {'class': 'form-control'}}) }}

									<label class="custom-label" for="floatingInput" class="birthdate-label">Date Of Birth</label>
								</div>
								<div class="label-width xx form-floating">
									{{ form_widget(form.phonenumber,{'attr':{'class':'form-control'}}) }}
									{{ form_errors(form.phonenumber) }}
									<label class="custom-label" for="floatingInput">Phone number</label>
								</div>
							</div>
							<div class="margin-inputs form-floating">
								{{ form_widget(form.email,{'attr':{'class':'form-control'}}) }}
								{{ form_errors(form.email) }}
								<label class="custom-label" for="floatingInput">Email Adress</label>
							</div>
							<div class="margin-inputs form-floating">
								{{ form_widget(form.image,{'attr':{'class':'form-control'}}) }}

								<label class="custom-label" for="floatingInput">Profile image</label>
							</div>
							<div class="margin-inputs form-floating">
								{{ form_widget(form.password.first,{'attr':{'class':'form-control'}}) }}

								<label class="custom-label" for="floatingInput">Password</label>
							</div>
							<div class="margin-inputs form-floating">
								{{ form_widget(form.password.second,{'attr':{'class':'form-control'}}) }}

								<label class="custom-label" for="floatingInput">Confirm Password</label>
							</div>
							<div class="margin-inputs label-width1">
								<button type="submit" class="form-control">Submit</button>
							</div>
							{{ form_end(form) }}
						</div>
					</div>

				</div>
			</div>
		</div>
	</div>
</section>{% endblock %}
